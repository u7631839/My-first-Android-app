<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="-45"
            android:pivotX="0%"
            android:pivotY="0"
            android:toDegrees="0">
            <shape
                android:shape="rectangle">
                <solid android:color="#E5FFCC" />
                <corners android:radius="100dp" />
            </shape>

        </rotate>

    </item>
    <!-- Inner white background -->
    <item android:right="8dp" android:left="8dp" android:top="8dp" android:bottom="8dp">
        <shape
            android:shape="rectangle">
            <solid android:color="#E5FFCC" />
            <corners android:radius="100dp" />
        </shape>
    </item>
</layer-list>